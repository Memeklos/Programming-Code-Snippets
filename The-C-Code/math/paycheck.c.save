#include <stdio.h>

int main() {

	float hrs;
	float gross;
	float tax;
	float input;

// Input \\
	printf("Hours worked: ");
	scanf("%f", &hrs);
	printf("Hourly pay: ");
	scanf("%f", &gross);
	pay = gross * hrs;
	printf("Tax percentage: ");
	scanf("%f", &tax);

// Output \\
	printf("Gross income: %f", pay);
	printf("Tax: %f");
	printf("Net income: %f");

	return 0;
}